# Project name to separate multiple bitrixdock instances
PROJECT=darbit_docker_

# PHP version settings. Allowed value is php74, php80, php81, php82, php83.
PHP_VERSION=#PHP_VERSION#

# Web server type. Allowed value is apache or nginx.
WEB_SERVER_TYPE=nginx

# MySQL settings. Allowed value is mysql or percona.
DB_SERVER_TYPE=mysql
MYSQL_DATABASE=bitrix
MYSQL_USER=bitrix
MYSQL_PASSWORD=#MYSQL_PASSWORD#
MYSQL_ROOT_PASSWORD=#DATABASE_ROOT_PASSWORD#

# Network
INTERFACE=0.0.0.0

# Site path
SITE_PATH=#WEBSITE_PATH#

DOMAIN_URL=#DOMAIN_URL#
DOMAIN_EMAIL=#DOMAIN_EMAIL#